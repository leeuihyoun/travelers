<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.exampl.traveler.mapper.BookingMapper">
    <insert id="insertBooking" parameterType="com.exampl.traveler.vo.BookingVO">
        INSERT INTO Booking (userId, airlineId, seatNo, status)
        VALUES (#{userId}, #{airlineId}, #{seatNo}, #{status})
    </insert>
</mapper>